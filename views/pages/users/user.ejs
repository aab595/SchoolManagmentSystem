<%- include('../partials/_header') %>

<%- include('../partials/_sidebar') %>
    <div class="main-content">
        <h1>User Page</h1>
        <table>
            <thead>
                <th>Name</th>
                <th>Email</th>
                <th>Profil</th>
                <th>Téléphone</th>
                <th>Adresse</th>
                <th>Genre</th>
            </thead>
            <tbody>
                <% users.forEach(user => { %>
                    <tr>
                        <td><%= user.name %> </td>
                        <td><%= user.email %> </td>
                        <td><%= user.profil %> </td>
                        <td><%= user.phone %> </td>
                        <td><%= user.address %> </td>
                        <td><%= user.gender %> </td>
                    </tr>
                <% }) %> 
            </tbody>
        </table>
    </div>
<%- include('../partials/_footer') %>